{
  "name": "minifit",
  "productName" : "Minifit",
  "version": "1.1.5",
  "description": "Minifit is a JavaScript compressor, beutify and compiler for ES6",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "buildLinux": "electron-packager . minifit --overwrite --platform=linux --arch=x64 --icon=App/icons/icon.png --prune=true --out=release-builds",
    "buildWin": "electron-packager . minifit --overwrite --platform=win32 --arch=ia32 --icon=App/icons/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='Minifit'",
    "buildMac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=App/icons/icon.icns --prune=true --out=release-builds",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "BlobySoftware",
  "license": "MIT",
  "dependencies": {
    "bulma": "^0.7.2",
    "drag-drop": "^4.2.0",
    "geckojs": "^1.2.55",
    "uglify-es": "^3.3.9"
  },
  "keywords":[
    "minify",
    "compression",
    "uglifyjs",
    "beutify",
    "compiler",
    "drag",
    "drop",
    "minifit",
    "geckojs",
    "es6"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/BlobySoftware/Minifit"
  },
  "devDependencies": {
    "electron": "^4.0.0",
    "electron-packager": "^13.0.1"
  }
}
